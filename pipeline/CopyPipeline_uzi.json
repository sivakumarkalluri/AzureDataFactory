{
	"name": "CopyPipeline_uzi",
	"properties": {
		"activities": [
			{
				"name": "ForEach_uzi",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_uzi",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "dbo.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "pharmaclaims//@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings",
										"maxConcurrentConnections": 10,
										"blockSizeInMB": 10
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_uzi",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_uzi",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "Claim_Details"
						},
						"destination": {
							"fileName": "Claim_Details.txt"
						}
					},
					{
						"source": {
							"table": "Diagnosis_details"
						},
						"destination": {
							"fileName": "Diagnosis_details.txt"
						}
					},
					{
						"source": {
							"table": "Member_Details"
						},
						"destination": {
							"fileName": "Member_Details.txt"
						}
					},
					{
						"source": {
							"table": "Member_PCP"
						},
						"destination": {
							"fileName": "Member_PCP.txt"
						}
					},
					{
						"source": {
							"table": "Place_of_Service"
						},
						"destination": {
							"fileName": "dbo.Place_of_Service.txt"
						}
					},
					{
						"source": {
							"table": "Procedure_details"
						},
						"destination": {
							"fileName": "dbo.Procedure_details.txt"
						}
					},
					{
						"source": {
							"table": "Provider_Details"
						},
						"destination": {
							"fileName": "dbo.Provider_Details.txt"
						}
					}
				]
			}
		},
		"annotations": []
	}
}